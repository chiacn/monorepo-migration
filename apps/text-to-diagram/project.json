{
  "name": "text-to-diagram",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/text-to-diagram/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/text-to-diagram"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "text-to-diagram:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "text-to-diagram:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "text-to-diagram:build:production",
          "dev": false
        }
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "text-to-diagram:build:production",
        "dev": false
      },
      "configurations": {
        "production": {
          "buildTarget": "text-to-diagram:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "outputs": ["{options.outputPath}"],
      "options": {
        "buildTarget": "text-to-diagram:build:production",
        "outputPath": "dist/apps/text-to-diagram/export"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/text-to-diagram/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
